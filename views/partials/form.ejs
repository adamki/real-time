<form action="/polls" method="POST" name="poll" class="col-md-6">
  <fieldset class="form-group">
    <div class="input-field col s12">
      <input name="poll[title]" id="poll-title" type="text" placeholder="Cloud Fund" class="form-control">
    </div>
    <div class="input-field col s12">
      <input name="poll[responses][]" id="choice1" type="text" placeholder="Response 1" class="form-control">
    </div>
    <div class="input-field col s12">
      <input name="poll[responses][]" id="choice2" type="text" placeholder="Response 2" class="form-control">
    </div>
  </fieldset>

    <a href="#" id="addField"><span class="glyphicon glyphicon-plus" aria-hidden="true"></span> </a>
  <button class="btn waves-effect waves-light btn pull-right" type="submit">Submit</button>
</form>
